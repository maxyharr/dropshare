%header.navbar.navbar-fixed-top.navbar-inverse
  .navbar-inner
    .container
      = link_to "Dropshare", '/', id: "logo"
      %nav
        %ul.nav.pull-right
          %li= link_to "Home", root_path, id: "home"
          %li= link_to "Help", '/help', id: "help"
          - if authenticated?
            %li= link_to "Sign out", facebook_path, :method => :delete
            %li= link_to current_user, wall_path, id: "wall_link"
